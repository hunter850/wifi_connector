{
    "name": "electron_webpack_ts_empty",
    "version": "1.0.0",
    "description": "electron webpack ts empty",
    "main": "./build/electron/main.js",
    "author": "hunterluo",
    "scripts": {
        "dev": "yarn electron-parse-ts && concurrently \"cross-env MODE=development webpack serve --mode=development\" \"wait-on http://127.0.0.1:3000 && electron-forge start\"",
        "start": "yarn electron-parse-ts && concurrently \"cross-env MODE=development webpack serve --mode=development\" \"wait-on http://127.0.0.1:3000 && electron-forge start\"",
        "build": "cross-env NODE_ENV=production webpack --mode=production && yarn electron-parse-ts && electron-forge make",
        "electron-parse-ts": "tsc -p tsconfig.node.json",
        "web-build": "cross-env NODE_ENV=production webpack --mode=production"
    },
